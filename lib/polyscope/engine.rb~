module Polyscope
  class Engine < ::Rails::Engine
    isolate_namespace Polyscope
	config.generators do |g|
      	g.test_framework      :rspec,        :fixture => false
      	g.fixture_replacement :factory_girl, :dir => 'spec/factories'
      	g.assets false
      	g.helper false
	g.orm :active_record
    	end
    
  end
end
